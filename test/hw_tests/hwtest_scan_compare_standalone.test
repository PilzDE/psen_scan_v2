<launch>
  <arg name="test_duration" default="10"/>

  <test test-name="scan_compare_standalone" pkg="psen_scan_v2" type="hwtest_scan_compare_standalone"
        time-limit="$(eval test_duration + 10)">
    <param name="testfile" value="$(env HW_TEST_SCAN_COMPARE_TESTFILE)"/>
    <param name="test_duration" value="$(arg test_duration)"/>
    <param name="angle_start" value="68.7"/>
    <param name="angle_end" value="206.3"/>
    <param name="host_ip" value="192.168.0.50"/>
    <param name="scanner_ip" value="192.168.0.10"/>
  </test>
</launch>
